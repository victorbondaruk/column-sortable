{
  "name": "victorbondaruk/column-sortable",
  "description": "Package for handling column sorting in Laravel 7.x",
  "keywords": [
    "sortable",
    "sorting",
    "column",
    "sort",
    "laravel"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Victor Bondaruk",
      "email": "bondaruk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "5.8.*|^6.0|^7.0",
    "illuminate/database": "5.8.*|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "VictorBondaruk\\ColumnSortable\\": "src/ColumnSortable/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VictorBondaruk\\ColumnSortable\\ColumnSortableServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
